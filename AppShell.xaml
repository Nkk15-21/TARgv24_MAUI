<?xml version="1.0" encoding="utf-8" ?>
<Shell
    x:Class="TARgv24.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:TARgv24"
    Title="TARgv24">

    <TabBar>
        <ShellContent Title="Start"      ContentTemplate="{DataTemplate pages:StartPage}"       Route="start" />
        <!--<ShellContent Title="Main"       ContentTemplate="{DataTemplate pages:MainPage}"        Route="main" />-->
        <ShellContent Title="Valgusfoor" ContentTemplate="{DataTemplate pages:ValgusfoorPage}"  Route="valgusfoor" />
        <ShellContent Title="Timer"      ContentTemplate="{DataTemplate pages:TimerPage}"       Route="timer" />
        <ShellContent Title="Lumememm"   ContentTemplate="{DataTemplate pages:LumememmPage}"    Route="lumememm" />
        <ShellContent Title="Figure"     ContentTemplate="{DataTemplate pages:FigurePage}"      Route="figure" />
        <ShellContent Title="Text"       ContentTemplate="{DataTemplate pages:TekstPage}"       Route="tekst" />
        <ShellContent Title="Date/Time"  ContentTemplate="{DataTemplate pages:DateTimePage}"    Route="datetime" />
    </TabBar>
</Shell>
