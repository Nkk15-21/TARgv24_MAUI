<?xml version="1.0" encoding="utf-8" ?>
<Shell
    x:Class="TARgv24.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TARgv24"

    
    <!-- Главная страница приложения -->
    <TabBar>
        <ShellContent
            Title="Avaleht"
            Route="start"
            ContentTemplate="{DataTemplate local:StartPage}" />

        <ShellContent
            Title="Lumememm"
            ContentTemplate="{DataTemplate local:LumememmPage}"
            Route="lumememm" />
        
        <ShellContent 
            Title="Game" ContentTemplate="{DataTemplate views:GamePage}" />
        
        <ShellContent 
            Title="Results" ContentTemplate="{DataTemplate views:ResultsPage}" />
        <ShellContent 
            Title="Settings" ContentTemplate="{DataTemplate views:SettingsPage}" />
        

    </TabBar>


</Shell>
